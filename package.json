{
  "name": "tgol",
  "version": "0.0.1",
  "description": "Several cell-configurations compete in a tournament; Inspired by Conway's Game of Life",
  "main": "index.js",
  "scripts": {
    "prepublish": "coffee --compile --output lib src && npm run copy-dist && npm run browserify && node mark-deps-bundled.js",
    "test": "mocha spec --recursive --require spec/helpers/chai.js --require coffee-script/register --compilers coffee:coffee-script/register",
    "test-debug": "node-debug node_modules/mocha/bin/_mocha  --recursive spec --require spec/helpers/chai.js --require coffee-script/register --compilers coffee:coffee-script/register --no-timeouts --debug-brk",
    "test-watch": "mocha -b --recursive -w spec --require spec/helpers/chai.js --require coffee-script/register --compilers coffee:coffee-script/register",
    "test-jenkins": "JUNIT_REPORT_PATH=report.xml JUNIT_REPORT_STACK=1 mocha spec --recursive --require spec/helpers/chai.js --timeout 15000 --reporter mocha-jenkins-reporter --require coffee-script/register --compilers coffee:coffee-script/register||true",
    "start": "coffee src/main.coffee",
    "browserify": "mkdir -p dist/js && browserify -e lib/client/index.js  -o dist/js/client.js",
    "copy-dist": "mkdir -p dist && cp -r static/* dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldegen/tgol.git"
  },
  "author": "Lukas Degener <l.degener@tarent.de>",
  "license": "MirOS",
  "bugs": {
    "url": "https://github.com/ldegen/tgol/issues"
  },
  "homepage": "https://github.com/ldegen/tgol#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "coffee-script": "^1.10.0",
    "dom-storage": "^2.0.2",
    "mocha": "^2.5.3",
    "mocha-jenkins-reporter": "^0.2.3",
    "node-inspector": "^0.12.8",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "baconjs": "^0.7.84",
    "bluebird": "^3.4.1",
    "browserify-middleware": "^7.0.0",
    "bulk-require": "^1.0.0",
    "coffeeify": "^2.0.1",
    "d3-brush": "^1.0.1",
    "d3-drag": "^1.0.0",
    "d3-selection": "^1.0.0",
    "d3-zoom": "^1.0.2",
    "deepmerge": "^0.2.10",
    "document-ready": "^1.0.2",
    "express": "^4.14.0",
    "js-yaml": "^3.6.1",
    "kbpgp": "^2.0.54",
    "mkdirp": "^0.5.1",
    "qr-image": "^3.1.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "readdirp": "^2.1.0",
    "rimraf": "^2.5.3"
  },
  "bundleDependencies": [
    "baconjs",
    "bluebird",
    "browserify-middleware",
    "bulk-require",
    "coffeeify",
    "d3-brush",
    "d3-drag",
    "d3-selection",
    "d3-zoom",
    "deepmerge",
    "document-ready",
    "express",
    "js-yaml",
    "kbpgp",
    "mkdirp",
    "qr-image",
    "react",
    "react-dom",
    "react-router",
    "readdirp",
    "rimraf"
  ]
}
